loadplugins '\.(js|penta)$'
group user
map w -builtin 10h
map e -builtin 10l
map h -builtin <count><C-p>
map j -builtin <count>3<Down>
map k -builtin <count>3<Up>
map l -builtin <count><C-n>
map e -ex set go!=TB
map ! -ex set usermode!
map <BS> H
set go=bCrs
set hlfind
set passkeys+=mail.google.com:j,k,<SPACE>,x,I,?,<CR>,s,y
set nextpattern+='^次'
set previouspattern+='^前'
set followhints=1

style -name=cmd-ime chrome://*
    \ #dactyl-commandline-command input,
    \ #dactyl-statusline-field-commandline-command input {
    \     ime-mode: inactive;
    \ }

"cmap <A-i> -js styles.user.get("cmd-ime").enabled ^= true; dactyl.focusedElement.blur();

" vim: set ft=vim:
